.background1{
  width: 100%;
  height: 600rpx;
  margin: 0;
}
.background1 image{
  width: 100%;
  height: 600rpx;
}
.background2{
  top:329rpx;
  width: 100%;
  height: 600rpx;
  margin: 0;
  position: absolute;
}
.background2 image{
  position: absolute;
  width: 100%;
  height: 600rpx;
}

.no1_box{
  top: 360rpx;
  width: 300rpx;
  height: 440rpx;
  left:50rpx;
  position: absolute;
  align-items: center;
  justify-content: center;
  border-radius: 8px;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 11px 28px 0px rgba(0, 0, 0, 0.29);
}
.no1_box image{
  width: 90%;
  top:15rpx;
  position: relative;
  margin: auto;
  /* align-items: center; */
  /* justify-content: center; */
  display: flex;
}
.no1_box text{
  bottom:-20rpx;
  left:20rpx;
  position: relative;
  color: #9c9c9c;
}

